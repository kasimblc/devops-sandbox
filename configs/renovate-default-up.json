{
    "extends": ["github>LivasoftConnector/renovate-config:configs/renovate-core.json"],
    "dependencyDashboard": false,
    "masterIssue": true,
    "branchPrefix": "renovate-",
    "separateMajorMinor": false,
    "ignoreDeps": [],
    "rebaseLabel": "renovate-rebase",
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0,
    "labels": [
        "update-package"
    ],
    "assignees": [
        "@firatoltulu"
    ],
    "hostRules": [
        {
            "hostType": "nuget",
            "matchHost": "https://nuget.pkg.github.com",
            "username": "firatoltulu",
            "encrypted": {
                "password": ""
            }
        }
    ],
    "packageRules": [
        {
        "packagePatterns": [
            "*"
        ],
        "enabled": false
        },
        {
        "matchPackagePrefixes": [
            "ShopExpress"
        ],
        "groupName": "shopexpress",
        "pinVersions": false,
        "enabled": true,
        "automerge": true
        }
    ],
    "nuget": {
        "enabled": false
    },
    "npm": {
        "enabled": false
    },
    "commitMessage": "chore(norelease): update dependency {{depName}} to {{newValue}}"
}